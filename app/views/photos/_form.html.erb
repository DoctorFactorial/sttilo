<%= simple_form_for(@photo, html: {class: 'form-signin'}) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, label: false, placeholder: 'Name' %>
     <%= f.input :category, label: false, collection: Subpage.all.pluck(:slug), prompt: "Select subpage" %>
    
    <%= f.input :image, label:false %>
  
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => "btn-primary btn-block" %>
  </div>
<% end %>
