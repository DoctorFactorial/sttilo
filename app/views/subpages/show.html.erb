<div class='container-fluid'>
<div class='row'>
	<div class='col-md-2'></div>
	<div class='col-md-8'>
		<div>
			<h2 class='blog_post_title text-center'><%= @subpage.name %></h2>
			<p class='blog_date_subpages'><%= I18n.l @subpage.created_at %></p>
			<% if @subpage.content_html %>
			<%= raw RedCloth.new(@subpage.content_html).to_html %>
			<% end %>
			<%= render 'pages/photos' %>
		</div>
 	</div>
 	<br>
 	<div class='col-md-2'></div>
 </div>
 
 <div class='row'>
	<div class="text-center links">
		<% if current_user.try(:admin?) %>
		<%= link_to 'Edit', edit_subpage_path(@subpage) %> |
		<% end %>
		<%= link_to 'Back', url_for(:back) %>
 	</div>
 </div>
</div>