<h3 class='text-center'><%= link_to 'Add Photo', new_photo_path, class: 'btn btn-primary btn-sm' %></h3>
<%= form_tag(admin_photos_path, :method => "get", id: "search-form") do %>
	<%= text_field_tag :search, params[:search], placeholder: "Search" %>
	<%= submit_tag "Search", :name => nil %>
<% end %>

<%= render 'datatable' %>
