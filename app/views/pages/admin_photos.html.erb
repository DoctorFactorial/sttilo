<h3 class='text-center'><%= link_to 'Add Photo', new_photo_path, class: 'btn btn-primary btn-sm' %></h3>

<div class='col-md-10 col-md-offset-1 text-right'>
<%= form_tag(admin_photos_path, :method => "get", id: "search-form") do %>
<%= text_field_tag :search, params[:search], placeholder: "Search for...", class: "form-control" %>
<%= submit_tag "Search", :name => nil, class: "btn btn-small" %>
<% end %>
</div>

<%= render 'datatable' %>
